                ],
                "view": "timeSeries",
                "stacked": false,
                "region": "{{YOUR_AWS_REGION}}",
                "title": "Output Retry/Failed Rates",
                "stat": "Sum",
                "period": 300,
                "legend": {
                    "position": "hidden"
                },
                "yAxis": {
                    "left": {
                        "showUnits": false
                    }
                }
            }
